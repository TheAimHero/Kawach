{
  "author": "Team EOS",
  "browser_action": {
    "browser_style": false,
    "default_title": "TBD",
    "default_popup": "html/popup.html"
  },
  "manifest_version": 2,
  "name": "TBD",
  "options_ui": {
    "open_in_tab": true
  },
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png"
  },
  "background": {
    "scripts": [
      "dist/background.bundle.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content_script.bundle.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "privacy",
    "storage",
    "tabs",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "TBD",
  "version": "0.0.1"
}
