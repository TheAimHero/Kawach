{
  "author": "Team EOS",
  "browser_action": {
    "browser_style": false,
    "default_title": "TBD",
    "default_popup": "html/popup.html"
  },
  "_version": "0.0.1",
  "manifest_version": 2,
  "name": "TBD",
  "options_ui": {
    "open_in_tab": true
  },
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "64": "img/64.png"
  },
  "background": {
    "scripts": ["dist/background.bundle.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content_script.bundle.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "host_permissions": ["<all_urls>"],
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },

  "options_page": "build/index.html",

  "permissions": [
    "privacy",
    "storage",
    "tabs",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": ["model/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "short_name": "TBD",
  "version": "0.0.1"
}
